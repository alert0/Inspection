<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
			  android:layout_width="wrap_content"
			  android:layout_height="wrap_content"
			  android:orientation="horizontal"
			  android:padding="@dimen/x10">
	
	<ImageView
		android:id="@+id/iv_item_brand_detail_chengban"
		android:layout_width="@dimen/x40"
		android:layout_height="@dimen/x50" />
	
	<LinearLayout
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginLeft="@dimen/x10"
		android:orientation="vertical">
		
		<TextView
			android:id="@+id/tv_item_brand_detail_chengban_title"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:textColor="@color/gray22"
			android:textSize="@dimen/t14" />
		
		<TextView
			android:id="@+id/tv_item_brand_detail_chengban_name"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:textColor="@color/gray26"
			android:textSize="@dimen/t16" />
	</LinearLayout>
</LinearLayout>